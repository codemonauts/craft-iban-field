{
  "name": "codemonauts/craft-iban-field",
  "description": "Craft CMS plugin to store, validate and format IBAN codes.",
  "version": "2.0.0",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "IBAN"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "codemonauts",
      "homepage": "https://codemonauts.com"
    }
  ],
  "support": {
    "source": "https://github.com/codemonauts/craft-iban-field",
    "docs": "https://github.com/codemonauts/craft-iban-field/blob/master/README.md",
    "issues": "https://github.com/codemonauts/craft-iban-field/issues"
  },
  "require": {
    "craftcms/cms": "^4.0.0",
    "jschaedl/iban-validation": "^1.5.0"
  },
  "autoload": {
    "psr-4": {
      "codemonauts\\iban\\": "src/"
    }
  },
  "extra": {
    "handle": "iban",
    "class": "codemonauts\\iban\\Iban",
    "name": "IBAN Field"
  }
}
